<%= form_for(@evento) do |f| %>
  <%= f.label :nome %>: <%= f.text_field :nome %><br/>
  <%= f.label :tipo %>: <%= f.text_field :tipo %><br/>
  <%= f.label :local %>: <%= f.text_field :local %><br/>
  <%= f.label :custo %>: <%= f.text_field :custo %><br/>
  <%= f.label :data %>: <%= f.datetime_select :data %><br/>
  <%= f.label :image %>: <%= f.text_field :image %><br/>
  <%= f.submit %>
<% end %>
